  inherited up: TZUpdateSQL
    DeleteSQL.Strings = (
      'DELETE FROM produto'
      'WHERE'
      
        '  ((produto.Id IS NULL AND :OLD_Id IS NULL) OR (produto.Id = :OL' +
        'D_Id))')
    InsertSQL.Strings = (
      'INSERT INTO produto'
      '  (produto.descricao, produto.tipo, produto.est_seguranca, '
      'produto.est_minimo, '
      
        '   produto.ft_conv_un, produto.ft_conv_qtde, produto.ft_conv_2un' +
        ', '
      'produto.produzido, '
      
        '   produto.barras, produto.referencia, produto.valor, produto.qt' +
        'destoque)'
      'VALUES'
      '  (:descricao, :tipo, :est_seguranca, :est_minimo, :ft_conv_un, '
      ':ft_conv_qtde, '
      
        '   :ft_conv_2un, :produzido, :barras, :referencia, :valor, :qtde' +
        'stoque)')
    ModifySQL.Strings = (
      'UPDATE produto SET'
      '  produto.descricao = :descricao,'
      '  produto.tipo = :tipo,'
      '  produto.est_seguranca = :est_seguranca,'
      '  produto.est_minimo = :est_minimo,'

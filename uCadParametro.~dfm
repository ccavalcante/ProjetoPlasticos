  inherited up: TZUpdateSQL
    DeleteSQL.Strings = (
      'DELETE FROM parametro'
      'WHERE'
      
        '  ((parametro.id IS NULL AND :OLD_id IS NULL) OR (parametro.id =' +
        ' '
      ':OLD_id))')
    InsertSQL.Strings = (
      'INSERT INTO parametro'
      '  (parametro.id,parametro.RAZAO, parametro.CNPJ, '
      'parametro.ENDERECO, parametro.BAIRRO, '
      
        '   parametro.CIDADE, parametro.UF, parametro.CEP, parametro.SITE' +
        ', '
      'parametro.EMAIL, '
      '   parametro.RESPONS, parametro.TELEFONE, parametro.FAX, '
      'parametro.logo, '
      
        '   parametro.ex, parametro.fantasia, parametro.num, parametro.id' +
        'Cidade, '
      '   parametro.MenuAtualizado)'
      'VALUES'
      '  (:ID, :RAZAO, :CNPJ, :ENDERECO, :BAIRRO, :CIDADE, :UF, :CEP, '
      ':SITE, :EMAIL, '
      
        '   :RESPONS, :TELEFONE, :FAX, :logo, :ex, :fantasia, :num, :idCi' +
        'dade, '
      ':MenuAtualizado)')

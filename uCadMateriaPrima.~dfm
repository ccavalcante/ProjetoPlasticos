    InsertSQL.Strings = (
      'INSERT INTO produto'
      
        '  (produto.descricao, produto.tipo, produto.est_seguranca, produ' +
        'to.est_minimo, '
      
        '   produto.ft_conv_un, produto.ft_conv_qtde, produto.ft_conv_2un' +
        ', produto.produzido, '
      
        '   produto.barras, produto.referencia, produto.valor, produto.qt' +
        'destoque)'
      'VALUES'
      
        '  (:descricao, :tipo, :est_seguranca, :est_minimo, :ft_conv_un, ' +
        ':ft_conv_qtde, '
      
        '   :ft_conv_2un, :produzido, :barras, :referencia, :valor, :qtde' +
        'stoque)')

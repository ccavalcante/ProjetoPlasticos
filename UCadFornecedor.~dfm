  inherited T: TZQuery
    AfterScroll = TAfterScroll
    OnNewRecord = TNewRecord
    SQL.Strings = (
      'select * from Fornecedor'
      'where (nome like :pesq  or fantasia like :pesq )'
      ''
      'order by nome')

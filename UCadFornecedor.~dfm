  inherited up: TZUpdateSQL
    DeleteSQL.Strings = (
      'DELETE FROM Fornecedor'
      'WHERE'
      
        '  ((Fornecedor.ID IS NULL AND :OLD_ID IS NULL) OR (Fornecedor.ID' +
        ' = '
      ':OLD_ID))')
    InsertSQL.Strings = (
      'INSERT INTO Fornecedor'
      '  (Fornecedor.ID, Fornecedor.NOME, Fornecedor.ENDERECO, '
      'Fornecedor.BAIRRO, '
      
        '   Fornecedor.CIDADE, Fornecedor.CEP, Fornecedor.UF, Fornecedor.' +
        'tel1, '
      'Fornecedor.tel2, '
      '   Fornecedor.fax, Fornecedor.CONTATO, Fornecedor.EMAIL, '
      'Fornecedor.IE, '
      '   Fornecedor.CNPJ, Fornecedor.CONTA, Fornecedor.AGENCIA, '
      'Fornecedor.BANCO, '
      '   Fornecedor.TIPOPAGTO, Fornecedor.OBS, Fornecedor.EX, '
      'Fornecedor.DESPESA, '
      '   Fornecedor.SITE, Fornecedor.TIPOPESSOA, Fornecedor.fantasia)'
      'VALUES'
      
        '  (:ID, :NOME, :ENDERECO, :BAIRRO, :CIDADE, :CEP, :UF, :tel1, :t' +
        'el2, '
      ':fax, '
      '   :CONTATO, :EMAIL, :IE, :CNPJ, :CONTA, :AGENCIA, :BANCO, '
      ':TIPOPAGTO, '
      '   :OBS, :EX, :DESPESA, :SITE, :TIPOPESSOA, :fantasia)')
    ModifySQL.Strings = (
      'UPDATE Fornecedor SET'
      '  Fornecedor.ID = :ID,'
      '  Fornecedor.NOME = :NOME,'
      '  Fornecedor.ENDERECO = :ENDERECO,'

  object qProcItem: TZQuery
    Connection = DM.Conex
    UpdateObject = upProcItem
    SQL.Strings = (
      'select processo_I.*, produto.descricao'
      ' from processo_I'
      
        'left join produto on processo_I.idproduto = produto.id and produ' +
        'to.tipo='#39'M'#39
      'where processo_I.idProcesso = :IDp')

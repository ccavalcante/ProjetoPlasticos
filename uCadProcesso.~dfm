  object upProcItem: TZUpdateSQL
    DeleteSQL.Strings = (
      'DELETE FROM processo_I'
      'WHERE'
      
        '  ((processo_I.Id IS NULL AND :OLD_Id IS NULL) OR (processo_I.Id' +
        ' = :OLD_Id))')
    InsertSQL.Strings = (
      'INSERT INTO processo_I'
      
        '  (processo_I.IdProcesso, processo_I.IdProduto, processo_I.Unida' +
        'de)'
      'VALUES'
      '  (:IdProcesso, :IdProduto, :Unidade)')
    ModifySQL.Strings = (
      'UPDATE processo_I SET'
      '  processo_I.IdProcesso = :IdProcesso,'
      '  processo_I.IdProduto = :IdProduto,'
      '  processo_I.Unidade = :Unidade'
      'WHERE'
      
        '  ((processo_I.Id IS NULL AND :OLD_Id IS NULL) OR (processo_I.Id' +
        ' = '
      ':OLD_Id))')
    UseSequenceFieldForRefreshSQL = False
    Left = 618
    Top = 88
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'IdProcesso'

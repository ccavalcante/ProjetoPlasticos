  inherited up: TZUpdateSQL
    DeleteSQL.Strings = (
      'DELETE FROM usuario'
      'WHERE'
      
        '  ((usuario.Id IS NULL AND :OLD_Id IS NULL) OR (usuario.Id = :OL' +
        'D_Id))')
    InsertSQL.Strings = (
      'INSERT INTO usuario'
      
        '  (usuario.Id, usuario.Nome, usuario.Senha, usuario.Data, usuari' +
        'o.User, '
      'usuario.Tipo, '
      
        '   usuario.Afastamento, usuario.CPF, usuario.RG, usuario.Nascime' +
        'nto, '
      'usuario.Endereco, '
      
        '   usuario.Numero, usuario.Bairro, usuario.Cidade, usuario.Cep, ' +
        'usuario.UF, '
      
        '   usuario.telefone, usuario.celular, usuario.fax, usuario.email' +
        ', '
      'usuario.venc_ferias, '
      '   usuario.venc_exame)'
      'VALUES'
      
        '  (:Id, :Nome, :Senha, :Data, :User, :Tipo, :Afastamento, :CPF, ' +
        ':RG, '
      ':Nascimento, '
      
        '   :Endereco, :Numero, :Bairro, :Cidade, :Cep, :UF, :telefone, :' +
        'celular, '
      '   :fax, :email, :venc_ferias, :venc_exame)')
    ModifySQL.Strings = (
      'UPDATE usuario SET'
      '  usuario.Nome = :Nome,'
      '  usuario.Senha = :Senha,'
      '  usuario.Data = :Data,'
      '  usuario.User = :User,'
      '  usuario.Tipo = :Tipo,'
      '  usuario.Afastamento = :Afastamento,'
      '  usuario.CPF = :CPF,'
      '  usuario.RG = :RG,'
      '  usuario.Nascimento = :Nascimento,'
      '  usuario.Endereco = :Endereco,'
      '  usuario.Numero = :Numero,'
      '  usuario.Bairro = :Bairro,'
      '  usuario.Cidade = :Cidade,'
      '  usuario.Cep = :Cep,'
      '  usuario.UF = :UF,'
      '  usuario.telefone = :telefone,'
      '  usuario.celular = :celular,'
      '  usuario.fax = :fax,'
      '  usuario.email = :email,'
      '  usuario.venc_ferias = :venc_ferias,'
      '  usuario.venc_exame = :venc_exame'
      'WHERE'
      
        '  ((usuario.Id IS NULL AND :OLD_Id IS NULL) OR (usuario.Id = :OL' +
        'D_Id))')
